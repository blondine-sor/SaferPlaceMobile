{
  "expo": {
    "name": "SaferPlace",
    "slug": "SaferPlace",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/saferp_icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/saferp_icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "notification": {
      "icon": "./assets/images/notif_icon.png",
      "color": "#FFFFFF"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/saferp_icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.blond27.SaferPlace"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/saferp_icon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "fd37ea61-fedb-4fed-885b-f68837450de5"
      }
    }
  }
}
